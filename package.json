{
	"name": "mob-pro-timer",
	"publisher": "masakurapa",
	"displayName": "MobProTimer",
	"description": "mob programmimg timer",
	"version": "0.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/masakurapa/MobProTimer"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.start",
		"onCommand:extension.pause"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.start",
				"title": "MobProTimer: Start"
			},
			{
				"command": "extension.pause",
				"title": "MobProTimer: Pause"
			}
		],
		"configuration": [
			{
				"title": "MobProTimer",
				"properties": {
					"mobprotimer.interval": {
						"type": "integer",
						"default": 10,
						"description": "mob interval time (minutes)",
						"scope": "window"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
